{
  "name": "portfolio",
  "version": "0.1.0",
  "private": false,
  "license": "UNLICENSED",
  "homepage": "http://alan-hadyk.github.io/portfolio",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --passWithNoTests --env=jest-environment-jsdom-sixteen",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "clearMocks": true,
    "moduleNameMapper": {
      "roots": [
        "./src"
      ],
      "^<src>/(.*)$": "<rootDir>/src/$1",
      "^<assets>/(.*)$": "<rootDir>/src/assets/$1",
      "^<helpers>/(.*)$": "<rootDir>/src/helpers/$1",
      "^<hooks>/(.*)$": "<rootDir>/src/hooks/$1",
      "^<pages>/(.*)$": "<rootDir>/src/pages/$1",
      "^<styles>/(.*)$": "<rootDir>/src/styles/$1",
      "^<atoms>/(.*)$": "<rootDir>/src/UI/atoms/$1",
      "^<layout>/(.*)$": "<rootDir>/src/UI/layout/$1",
      "^<molecules>/(.*)$": "<rootDir>/src/UI/molecules/$1",
      "^<organisms>/(.*)$": "<rootDir>/src/UI/organisms/$1",
      "^<state>/(.*)$": "<rootDir>/src/state/$1",
      "^<templates>/(.*)$": "<rootDir>/src/UI/templates/$1"
    },
    "transform": {
      "\\.svg$": "<rootDir>/src/fileTransform.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^5.7.0",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "detect-browser": "^5.1.1",
    "intersection-observer": "^0.11.0",
    "ip": "^1.1.5",
    "jest-styled-components": "^7.0.2",
    "lodash": "^4.17.20",
    "masonic": "^3.3.10",
    "node-fetch": "^2.6.1",
    "polished": "^3.6.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-device-detect": "^1.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "shuffle-text": "^0.3.1",
    "styled-components": "^5.2.0",
    "typescript": "^4.0.3",
    "vivus": "^0.4.5"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "@types/detect-browser": "^4.0.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "@types/vivus": "^0.4.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "canvas": "^2.6.1",
    "craco-alias": "^2.1.1",
    "cypress": "5.2.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-typescript-sort-keys": "^1.5.0",
    "gh-pages": "^3.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jsdom": "^16.4.0",
    "react-test-renderer": "^16.13.1"
  }
}
