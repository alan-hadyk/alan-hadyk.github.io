{
  "name": "portfolio",
  "version": "0.1.0",
  "private": false,
  "homepage": "http://alan-hadyk.github.io/portfolio",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --passWithNoTests --env=jest-environment-jsdom-sixteen",
    "test:coverage": "craco test --coverage --env=jest-environment-jsdom-sixteen",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "clearMocks": true,
    "moduleNameMapper": {
      "roots": [
        "./src"
      ],
      "^<src>/(.*)$": "<rootDir>/src/$1",
      "^<assets>/(.*)$": "<rootDir>/src/assets/$1",
      "^<helpers>/(.*)$": "<rootDir>/src/helpers/$1",
      "^<hooks>/(.*)$": "<rootDir>/src/hooks/$1",
      "^<pages>/(.*)$": "<rootDir>/src/pages/$1",
      "^<styles>/(.*)$": "<rootDir>/src/styles/$1",
      "^<atoms>/(.*)$": "<rootDir>/src/UI/atoms/$1",
      "^<layout>/(.*)$": "<rootDir>/src/UI/layout/$1",
      "^<molecules>/(.*)$": "<rootDir>/src/UI/molecules/$1",
      "^<organisms>/(.*)$": "<rootDir>/src/UI/organisms/$1",
      "^<state>/(.*)$": "<rootDir>/src/state/$1",
      "^<templates>/(.*)$": "<rootDir>/src/UI/templates/$1"
    },
    "transform": {
      "\\.svg$": "<rootDir>/src/fileTransform.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.11",
    "detect-browser": "^5.0.0",
    "intersection-observer": "^0.11.0",
    "ip": "^1.1.5",
    "jest-styled-components": "^7.0.0",
    "lodash": "^4.17.19",
    "masonic": "^3.3.2",
    "node-fetch": "^2.6.0",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.1",
    "shuffle-text": "^0.3.0",
    "styled-components": "^5.0.0",
    "typescript": "~3.9.7",
    "vivus": "^0.4.5"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/detect-browser": "^4.0.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^26.0.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.1",
    "@types/vivus": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "craco-alias": "^2.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-react": "^7.18.2",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-typescript-sort-keys": "^1.3.0",
    "gh-pages": "^3.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jsdom": "^16.2.0",
    "react-test-renderer": "^16.12.0"
  }
}
